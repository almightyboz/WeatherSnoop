<%= form_for :session, url: session_path do |f| %>
  <div class="field">
    <%= f.label :username, class: "form-text" %><br>
    <%= f.text_field :username %>
  </div>
  <div class="field">
    <%= f.label :password, class: "form-text" %><br>
    <%= f.password_field :password %>
  </div>
  <div class="actions">
    <%= f.submit "Log In" %>
  </div>
<% end %>